    "eleventy:build": "npx @11ty/eleventy"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@11ty/eleventy-img": "^2.0.1",
    "@tailwindcss/typography": "^0.5.2",
    "@vercel/node": "^2.5.6",
    "autoprefixer": "^10.0.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.0.9",
    "tailwindcss": "^3.0.23"
  }
}
